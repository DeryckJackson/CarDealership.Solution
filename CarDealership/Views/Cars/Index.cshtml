<!DOCTYPE html>
<html>
  <head>
    <meta charset='utf-8'>
    <title>Title</title>
    <link rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css'>
  </head>
  <body>
    <h1>Cars Listings</h1>
    @using CarDealership.Models;
    @if (Model.Count == 0)
    {
      <p>There are no Cars listed.</p>
    }
    <ul>
      @foreach (Car car in Model)
      {
        <li>Make Model: @car.GetMakeModel()</li>
        <li>Price: $@car.GetPrice()</li>
        <li>Miles: @car.GetMiles()</li>
        <br>
      }
    </ul>
  </body>
</html>